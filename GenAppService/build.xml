        <javac debug="true" debuglevel="${debuglevel}" destdir="WebContent/WEB-INF/classes" includeantruntime="false"
               source="${source}" target="${target}">
            <src path="src"/>
            <src path=".apt_generated"/>
            <classpath refid="GenAppService.classpath"/>
        </javac>
    </target>
    <target name="war">
        <war destfile="output/GenAppService.war" webxml="WebContent/WEB-INF/web.xml" update="true">
            <classes dir="WebContent\WEB-INF\classes"/>
            <fileset dir="WebContent">
